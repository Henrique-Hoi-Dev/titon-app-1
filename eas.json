{
  "cli": {
    "version": ">= 10.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "yarn": "1.22.18",
      "credentialsSource": "remote",
      "node": "22.14.0",
      "environment": "production"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal"
    },
    "staging": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "buildType": "app-store"
      },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "buildType": "app-store"
      }
    }
  },
  "submit": {
    "staging": {
      "ios": {}
    },
    "production": {
      "ios": {}
    }
  }
}
